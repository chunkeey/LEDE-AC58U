};


&spi_0 { /* BLSP1 QUP1 */
	status = "ok";

	mx25l25635e@0 {
		status = "okay";

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition0@0 {
				label = "all";
				reg = <0x00000000 0x10000000>;
				read-only; /* for now */
			};
		};

	};
